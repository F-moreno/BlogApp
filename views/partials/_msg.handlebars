{{#if success_msg}}
    {{#each success_msg}}
        <div class="alert alert-success">{{this}}</div>
    {{/each}}
{{/if}}

{{#if error_msg}}
    {{#each error_msg}}
        <div class="alert alert-danger">{{this}}</div>
    {{/each}}
{{/if}}

{{#if error}}
    {{#each error}}
        <div class="alert alert-danger">{{this}}</div>
    {{/each}}
{{/if}}